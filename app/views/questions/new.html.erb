<%= form_for(@question)  do |f| %>
	<%= render 'shared/error_messages', object: f.object %>

	<%= f.label :question %>
	<%= f.text_field :question,  :cols => 125, :rows => 4  %>

	<%= f.label :answer %>
	<%= f.text_field :answer,  :cols => 140 %>

	<%= f.hidden_field :holder_id, :value => @holder_id %>

	<%= f.submit "Create Question" %>

<% end %>